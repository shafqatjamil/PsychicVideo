<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_faq_tos_pp"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context="avreye.mytarotadvisor.ui.FaqTosPp">
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/navbarbg"
        android:id="@+id/imageView"
        android:scaleType="fitStart"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:text="Credits"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/faq_title"
        android:textAppearance="@android:style/TextAppearance.Material.Widget.Toolbar.Title"
        android:textSize="24sp"
        android:textAlignment="center"
        android:typeface="sans"
        tools:text="Credits"
        app:layout_constraintLeft_toLeftOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintRight_toRightOf="@+id/imageView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.32999998" />

    <WebView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/activity_faq_tos_pp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="@+id/activity_faq_tos_pp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_faq_tos_pp"
        android:layout_marginBottom="16dp"
        android:id="@+id/faq_webview" />
</android.support.constraint.ConstraintLayout>
